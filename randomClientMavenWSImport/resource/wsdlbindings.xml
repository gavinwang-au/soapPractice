<jaxws:bindings 
    xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
	xmlns:jaxws="http://java.sun.com/xml/ns/jaxws" 
	xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
	xmlns:s="http://www.w3.org/2001/XMLSchema"
	wsdlLocation="http://wsf.cdyne.com/WeatherWS/Weather.asmx?WSDL">
	<jaxws:bindings 
	    node="wsdl:definitions/wsdl:types/s:schema[@targetNamespace='http://ws.cdyne.com/WeatherWS/']">
		<jaxb:schemaBindings>
			<jaxb:package name="gen.com.weatherws" />
		</jaxb:schemaBindings>
	</jaxws:bindings>
	<jaxws:package name="gen.com.weatherws" />
</jaxws:bindings>